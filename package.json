{
  "name": "regexpr",
  "version": "1.0.0",
  "description": "A regexpr collection",
  "main": "./dist/regexpr.js",
  "repository": {
    "url": "https://github.com/TsPowerOne/RegExpr.git"
  },
  "directories": {
    "doc": "doc",
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "setup": "npm install",
    "build": "tsc",
    "build-watch": "tsc --watch",
    "test-only": "mocha --require ts-node/register test/**/*.ts",
    "pretest": "npm run build",
    "test": "npm run test-only",
    "typedoc": "typedoc --out ./docs --mode modules --tsconfig ./tsconfig.json "
  },
  "keywords": [
    "regexpr",
    "collection"
  ],
  "author": "Luigi Senzamici (luigisenzamici@gmail.com)",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.1.6",
    "@types/mocha": "^5.2.5",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "ts-node": "^7.0.1",
    "typedoc": "^0.12.0",
    "typescript": "^3.1.1"
  }
}
