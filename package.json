{
  "name": "@tspower/regexpr",
  "version": "1.1.0",
  "description": "A regexpr collection",
  "main": "./lib/regexpr.js",
  "repository": {
    "url": "git+https://github.com/TsPowerOne/RegExpr.git"
  },
  "directories": {
    "doc": "doc",
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "setup": "npm install",
    "build": "tsc",
    "build-watch": "tsc --watch",
    "test-console": "mocha --require ts-node/register test/**/*.ts",
    "test-only": "mocha --require ts-node/register test/**/*.ts --reporter mocha-junit-reporter",
    "pretest": "npm run build",
    "test": "npm run test-only",
    "typedoc": "typedoc --out ./docs --mode modules --tsconfig ./tsconfig.json "
  },
  "keywords": [
    "regexpr",
    "collection"
  ],
  "author": "Luigi Senzamici (luigisenzamici@gmail.com)",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.0",
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "mocha": "^6.2.0",
    "ts-node": "^8.3.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.6.2",
    "mocha-junit-reporter": "^1.23.1"
  },
  "bugs": {
    "url": "https://github.com/TsPowerOne/RegExpr/issues"
  },
  "homepage": "https://github.com/TsPowerOne/RegExpr#readme",
  "dependencies": {},
  "publishConfig": {
    "access": "public"
  }
}
